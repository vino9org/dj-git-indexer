    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run mirror",
            "type": "python",
            "request": "launch",
            "program": "manage.py",
            "console": "integratedTerminal",
            "args": [
                "mirror",
                "--source",
                "gitlab",
                "--query",
                "securitybankph/",
                "--filter",
                "*",
                "--output",
                "/Users/lee/tmp/sgc",
                "--overwrite"
            ],
            "justMyCode": false
        },
        {
            "name": "Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        }
    ]
}
